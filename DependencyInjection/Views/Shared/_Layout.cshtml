<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DependencyInjection</title>
    
</head>
<body>
    <header>
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home- Without DI</a>
        <br />
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="OldDI">OLD Fashion DI</a>
        <br />
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="ScopedDI">Scoped</a>
        <br />
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="TransientDI">Transiant</a>
        <br />
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="SingletonDI">Singleton</a>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

</body>
</html>
